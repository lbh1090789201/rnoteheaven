
    <% provide :link, "sign_up"  %>
    <% provide :title, "用户登陆" %>
    <% provide :content, "注册"  %>
    <div class="edit_basic edit-work-experiences no-icon sessions">

        <%= simple_form_for(resource, :as => resource_name, :url => user_session_path) do |f| %>
            <%= f.input :username, label: '用户名', input_html: {class: "form-control"} %>
            <%= f.input :password, label: '密码',as: :password, input_html: {class: "form-control"} %>
            <%= f.button :submit, "登陆", class: "btn-save", style: "width: 100%" %>
        <% end %>

        <%= react_component 'Seeker' %>
    </div>

    <script type="text/javascript">
        fullScreen('.edit_basic');
    </script>
